{% extends 'base.jinja2' %}

{% block main %}
    <script>
        function resizeIframe(obj) {
            obj.style.height = obj.contentWindow.document.body.scrollHeight + 'px';
        }
    </script>

    <div id="docs">
        <iframe src="/static_docs" style="width: 100%; margin: 10px; margin-top: 40px;" frameborder="0" scrolling="no" onload="resizeIframe(this)"></iframe>
    </div>
{% endblock %}
