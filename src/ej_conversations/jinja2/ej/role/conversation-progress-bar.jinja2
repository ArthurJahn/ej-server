<div class="Progress-status">
    <div class="Progress-porcentage">
        {{porcentage}}%
    </div>
    <div class="Progress-bar">
        <div class="Progress-bar-border">
            <div class="Progress-bar-content" style="width:{{porcentage}}%">
            </div>
        </div>
    </div>
    {% if total != None %}
        <div class="Progress-absolute">
            <strong>{{voted}}</strong>/{{total}}
        </div>
    {% endif %}
</div>
